{
	"name": "keep-mac-light",
	"version": "0.0.12",
	"description": "keep mac light no down",
	"license": "MIT",
	"repository": "chinanf-boy/keep-mac-light",
	"author": {
		"name": "chinanf-boy",
		"email": "865501259@qq.com",
		"url": "llever.com"
	},
	"bin": "cli.js",
	"engines": {
		"node": ">=4"
	},
	"scripts": {
		"precommit": "lint-staged",
		"test": "nyc ava -v",
		"pub": "npm run test && npm run npmUp && npm run git",
		"npmUp": "npm version patch && npm publish",
		"git": "git push && git push --tags"
	},
	"size-limit": [
		{
			"path": "cli.js",
			"gzip": false
		}
	],
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --single-quote --trailing-comma es5 --write",
			"git add"
		]
	},
	"files": [
		"cli.js"
	],
	"keywords": [
		"cli-app",
		"cli",
		"mac",
		"keep-light"
	],
	"dependencies": {
		"brightness": "^3.0.0",
		"exit-hook": "^2.0.0",
		"meow": "^5.0.0",
		"reverse-what-time": "0.0.4",
		"two-log": "0.0.9",
		"what-time": "^0.0.2"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"execa": "^0.10.0",
		"lint-staged": "^7.0.0",
		"nyc": "^12.0.2",
		"prettier": "^1.12.1"
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		]
	}
}
